library(tidyverse)
library(dplyr)
library(openxlsx)
library(tidytext)
library(forcats)
library(lubridate)
library(data.table)
ufo <- data.table::fread("ufo-complete-geocoded-time-standardized.csv")
ufo$Seconds <- as.double(ufo$Seconds)
ufo$Shape <- as.factor(ufo$Shape)
ufo$State_Province <- as.factor(ufo$State_Province)
ufo$Country <-  as.factor(ufo$Country)
ufo$Latitude <- as.double(ufo$Latitude)
ufo$Longitude <- as.double(ufo$Longitude)
ufo$City <- as.factor(ufo$City)
ufo$Date <- as.Date(ufo$Date, format = "%m/%d/%Y")
ufo$Time <- as.ITime(ufo$Time, tz = "GMT")
ufo
ufo %>%
group_by(Country)%>%
filter(n() >= 200)%>%
ggplot() +
geom_bar(aes(x = Country, fill = Country))+
coord_flip()+
labs(title = "Reports by Country",
y = "Number of Specific Reports")+
theme(legend.position ="none")
